# two dir node split
mkdir -p /tmp/a/{b,c};
sleep 1;

# directory recusive add
mkdir -p /tmp/a/{b,c}/d/e/f/{g,h,i,j}/{k,l};
sleep 1;

# add some file
touch /tmp/a/c/d/e/f/g/{f1,f2,f3}
sleep 1;

# directory recursive remove
rm -Rf /tmp/a/c/d/e/f/g
sleep 1;
rm -Rf /tmp/a/c
sleep 1;

# directory move
mv /tmp/a/b/d/e/f/h /tmp/a/b
sleep 1;

# directory rename
mv /tmp/a/b /tmp/a/p
sleep 1;

# file add
touch /tmp/a/p/file;
sleep 1;

# file change
echo "content" > /tmp/a/p/file;
sleep 1;

# file remove
rm /tmp/a/p/file;
sleep 1;

